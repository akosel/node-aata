{
  "name": "aata",
  "version": "1.0.0",
  "description": "Node wrapper around AATA API",
  "main": "./index.js",
  "scripts": {
    "lint": "eslint ./src ./test",
    "build": "babel ./src --out-dir ./lib --copy-files",
    "clean": "rm -rf ./lib",
    "pretest": "npm run lint",
    "test": "babel-node test/*.js",
    "preversion": "npm test",
    "version:auto": "npm version $(conventional-recommended-bump --preset=angular)",
    "postversion": "git push --tags && git push",
    "release": "npm run version:auto && npm publish",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": "> 0.8"
  },
  "author": {
    "name": "Aaron Kosel",
    "email": "aaronjkosel@gmail.com",
    "url": "http://www.aaronkosel.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akosel/node-aata.git"
  },
  "keywords": [
    "ann",
    "arbor",
    "a2",
    "transit",
    "ride",
    "bus"
  ],
  "author": "Aaron Kosel",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-eslint": "^4.1.5",
    "babel-preset-es2015": "^6.1.18",
    "blue-tape": "^0.1.10",
    "conventional-recommended-bump": "0.0.3",
    "cz-conventional-changelog": "^1.1.4",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0"
  }
}
